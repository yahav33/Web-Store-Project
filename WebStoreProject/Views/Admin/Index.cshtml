@model List<User>
@inject IRepositoryUser userRepo


<table class="table table-striped table-dark container">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">User Name</th>
            <th scope="col">Level</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {

            @if (!(userRepo.GetUserFromCookie().UserId == Model[i].UserId))
            {
                <tr>
                    <th scope="row">@(i + 1)</th>
                    <td>@Model[i].FirstName</td>
                    <td>@Model[i].LastName</td>
                    <td>@Model[i].UserName</td>
                    <td>@Model[i].Level</td>
                    <td><a asp-route-UserID="@Model[i].UserId" asp-controller="Admin" asp-action="EditUser" class="btn btn-primary">Edit</a></td>

                </tr>
            }


        }

    </tbody>
</table>



